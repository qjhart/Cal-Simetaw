#! /usr/bin/make -f 
ifndef configure.mk
include configure.mk
endif

#####################################################################
# Monthly Mapset files
#####################################################################
define prism

.PHONY: prism
prism::m$1
.PHONY: m$1
m$1:${rast}/m$1

${rast}/m$1: ${prism-$1}
	r.in.arc input=${prism-$1} output=m$1 type=FCELL mult=0.01
endef




